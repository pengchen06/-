<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserinfoServlet</servlet-name>
  	<display-name>UserinfoServlet</display-name>
  	<description></description>
  	<servlet-class>userinfo.servlet.UserinfoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DownServlet</servlet-name>
  	<display-name>DownServlet</display-name>
  	<description></description>
  	<servlet-class>userinfo.servlet.DownServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserinfoServlet</servlet-name>
  	<url-pattern>/UserinfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DownServlet</servlet-name>
  	<url-pattern>/DownServlet</url-pattern>
  </servlet-mapping>
</web-app>
